import collections 

class DequeContainer:
    
    def _init_ (self):
        self.deque = collections.deque ()
        
    def this_append (self, element):
        self.element = element 
        self.deque.append (self.element) 
        
    def my_popleft (self):
        if not self.deque:
            print ("deque is empty")
        else:
            print (self.deque.popleft ())
            
deque = DequeContainer ()
deque.this_append(10)
deque.this_append(20)
deque.this_append(30)
deque.this_append("Good")
deque.this_append("job")
print(deque.deque)
deque.my_popleft()
deque.my_popleft()
deque.my_popleft()
deque.my_popleft()
deque.my_popleft()
deque.my_popleft()
